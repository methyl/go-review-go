{
  "manifest_version": 2,

  "name": "Go Review Go",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "permissions": [
    "http://localhost:3001/"
  ],

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["jquery.js", "underscore.js", "backbone.js", "github.js"],
      "css": ["github.css"]
    }
  ]
}
